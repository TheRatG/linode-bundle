{
    "name": "therat/linode-bundle",
    "description": "Linode API 4",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Pak",
            "email": "theratg@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "doctrine/collections": "^1.5",
        "guzzlehttp/guzzle": "~6.3",
        "phpdocumentor/reflection-docblock": "~4.0",
        "symfony/config": "~4.0",
        "symfony/console": "~4.0",
        "symfony/dependency-injection": "~4.0",
        "symfony/dotenv": "~4.0",
        "symfony/event-dispatcher": "~4.0",
        "symfony/framework-bundle": "~4.0",
        "symfony/http-kernel": "~4.0",
        "symfony/messenger": "~4.0",
        "symfony/property-info": "~4.0",
        "symfony/serializer": "~4.0",
        "symfony/yaml": "~4.0"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require-dev": {
        "symfony/var-dumper": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "TheRat\\LinodeBundle\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
